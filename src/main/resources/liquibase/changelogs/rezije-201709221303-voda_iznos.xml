<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="201709231304" author="ifarszky">
        <createSequence sequenceName="voda_iznos_seq" minValue="1" incrementBy="1" />

        <createTable tableName="voda_iznos">
            <column name="id" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="voda_iznos_pk"/>
            </column>
            <column name="fix_voda" type="DECIMAL(4, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="fix_odvodnja" type="DECIMAL(4, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="voda_po_metru" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="odvodnja_po_metru" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="pdv_osnovica" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="pdv" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="nak_za_koristenje" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="nak_za_zastitu" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="nak_za_razvoj" type="DECIMAL(5, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="ukupno" type="DECIMAL(6, 2)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>